#!/usr/bin/env python
import sys


def print_help():
    print('                                ')
    print('================================')
    print('Welcome to Pact Test for Python!')
    print('--------------------------------')
    print('       Available Commands       ')
    print('                                ')
    print('verify..........Verify Pact test')
    print('================================')
    print('                                ')


def verify():
    pass


if len(sys.argv) is 1:
    print_help()
else:
    if sys.argv[1] == 'verify':
        verify()
    else:
        print_help()
